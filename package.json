{
  "name": "@dotstudiopro/zapp-pipes-provider-dotstudiopro",
  "version": "1.0.0",
  "description": "Zapp Provider for Applicaster platform by dotstudioPRO",
  "main": "dist/index.js",
  "scripts": {
    "start": "nodemon --exec babel-node src/index.js",
    "build": "rimraf dist && cross-env NODE_ENV=production babel src -d dist",
    "lint": "eslint --fix src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "url": "git+ssh://git@github.com/dotstudiopro/zapp-pipes-provider-dotstudiopro.git",
    "type": "git"
  },
  "author": "dotstudioPRO",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dotstudiopro/zapp-pipes-provider-dotstudiopro/issues"
  },
  "homepage": "https://github.com/dotstudiopro/zapp-pipes-provider-dotstudiopro#readme",
  "type": "data_source_provider",
  "identifier": "dotstudioPRO",
  "ui_builder_support": true,
  "dependency_name": "dotstudioPRO",
  "dependency_version": "0.1.0",
  "min_zapp_sdk": "10.0.9",
  "deprecated_since_zapp_sdk": "",
  "unsupported_since_zapp_sdk": "",
  "scheme": "dotstudiopro",
  "data_types": [
    {
      "label": "category",
      "value": "string",
      "documentation": {
        "link": "https://api.myspotlight.tv/#tag/category",
        "input_description": "Category Slug",
        "input_placeholder": "Please type the category slug",
        "input_description_image_url": ""
      }
    },
    {
      "label": "channel",
      "value": "string",
      "documentation": {
        "link": "https://api.myspotlight.tv/#tag/channel",
        "input_description": "Channel Slug",
        "input_placeholder": "Please type the channel slug",
        "input_description_image_url": ""
      }
    },
    {
      "label": "video",
      "value": "string",
      "documentation": {
        "link": "https://api.myspotlight.tv/#tag/video",
        "input_description": "Video ID",
        "input_placeholder": "Please type the Video ID",
        "input_description_image_url": ""
      }
    },
    {
      "label": "https://api.myspotlight.tv/#tag/playlist",
      "value": "string",
      "documentation": {
        "link": "https://api.myspotlight.tv/#tag/playlist",
        "input_description": "Playlist ID",
        "input_placeholder": "Please type the Playlist ID",
        "input_description_image_url": ""
      }
    },
    {
      "label": "vmap",
      "value": "string",
      "documentation": {
        "link": "https://api.myspotlight.tv/#tag/vmap",
        "input_description": "VMAP URL",
        "input_placeholder": "Please add the video ID to get VMAP",
        "input_description_image_url": ""
      }
    }
  ],
  "files": [
    "dist"
  ],
  "babel": {
    "presets": [
      "es2015-node6"
    ],
    "env": {
      "production": {
        "plugins": [
          "add-module-exports"
        ],
        "presets": [
          "es2015",
          "babili"
        ]
      }
    }
  },
  "extends": "standard",
  "plugins": [
    "standard",
    "promise"
  ],
  "dependencies": {},
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-node6": "^0.4.0",
    "babili": "^0.1.4",
    "cross-env": "^5.2.1",
    "eslint": "6.4.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "rimraf": "^3.0.0"
  }
}
