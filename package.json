{
  "name": "zapp-pipes-provider-dotstudiopro",
  "version": "1.0.0",
  "description": "Zapp Provider for Applicaster platform by dotstudioPRO",
  "main": "dist/index.js",
  "scripts": {
    "start": "nodemon --exec babel-node src/index.js",
    "build": "rimraf dist && cross-env NODE_ENV=production babel src -d dist",
    "lint": "eslint --fix src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "dotstudioPRO",
  "license": "ISC",
  "files": [
    "dist"
  ],
  "babel": {
    "presets": [
      "es2015-node6"
    ],
    "env": {
      "production": {
        "plugins": [
          "add-module-exports"
        ],
        "presets": [
          "es2015",
          "babili"
        ]
      }
    }
  },
  "extends": "standard",
  "plugins": [
    "standard",
    "promise"
  ],
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-node6": "^0.4.0",
    "babili": "^0.1.4",
    "cross-env": "^5.2.1",
    "eslint": "6.4.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "rimraf": "^3.0.0"
  }
}
